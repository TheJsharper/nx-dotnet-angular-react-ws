{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "BuildingRestApiWithSbApplication",
            "request": "launch",
            "mainClass": "com.spring.academy.BuildingRestApiWithSbApplication",
            "projectName": "build-rest-api-with-sb"
        },
        {
            "type": "java",
            "name": "StartUpAppApplication",
            "request": "launch",
            "mainClass": "com.jsharper.start.up.app.StartUpAppApplication",
            "projectName": "start-up-app"
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/dist/backend/dotnet/libs/beyondPattern.Test/net7.0/Backend.Dotnet.Libs.Beyond.Pattern.Test.dll",
            "args": [],
            "cwd": "${workspaceFolder}/backend/dotnet/libs/beyondPattern.Test",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}